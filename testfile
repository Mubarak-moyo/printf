#include "main.h"

int main(void)
{
    char *str = "Hello, printf!";
    int count;

    count = _printf("This is a %s function.\n", "custom");
    _printf("Number of characters printed: %d\n", count);
    _printf("A char: %c\n", 'A');
    _printf("A percent sign: %%\n");
    _printf("String: %s\n", str);

    return 0;
}

